{
  "name": "webpack-ssr-razor-template-plugin",
  "version": "1.0.5",
  "description": "Plugin for webpack v4+ to generate templates using react ssr",
  "main": "dist/index.js",
  "repository": "https://www.github.com/nteague22/webpack-ssr-razor-template-plugin",
  "author": "nteague",
  "license": "MIT",
  "dependencies": {
    "glob": "^7.1.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "np": "^6.2.3",
    "typescript": "^3.9.2"
